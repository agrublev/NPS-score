        "@babel/plugin-syntax-class-properties": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/runtime": "^7.11.2",
        "cac": "^6.6.1",
        "nodemon": "^2.0.4",
